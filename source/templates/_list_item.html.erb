<script data-template="list" type="text/x-handlebars">
  <div class="sheep-list">
    <header class="sheep-list-header sheep-list-row">
      <span class="sheep-list-header-name">Navn</span>
      <span class="sheep-list-header-age">Alder</span>
      <span class="sheep-list-header-weight">Vekt</span>
    </header>
    <ul class="sheep-list-items">
      {{#collection}}
        <li class="sheep-list-item sheep-list-row">
          <span class="sheep-list-item-name">{{name}}</span>
          <span class="sheep-list-item-age">{{humanizeYears birthday}}</span>
          <span class="sheep-list-item-weight">{{weight}}</span>
        </li>
      {{/collection}}
    </ul>
  </div>
</script>